doctype 5
html(lang='ru')
	meta(charset="utf-8")
	title Some practice
	meta(name='viewport', content='width=device-width, initial-scale=1')
	link(href="https://fonts.googleapis.com/css2?family=Rubik:wght@300;400;500;700&display=swap" rel="stylesheet")
	style.
		html, body {
			height: 100%;
			width: 100%;
			margin: 0 !important;
			padding: 0 !important;
		}

		.container {
			min-height: 100%;
			min-width: 100%;
			margin: 0 !important;
			padding: 0 !important;
			
			background-color: #ffdfc3;
			background-image:url('data:image/png;base64,');
			background-size: cover;
			background-position: right;
		}
		.header,
		.mood-emoji-block__icon,
		.mood-emoji-block__times,
		footer{
			font-family: Rubik, serif;
		}
		.header {
			font-style: normal;
			font-weight: 700;
			font-size: 30px;
			line-height: 39px;
			color: #5c4e8e;
			padding: 20px 25px 0;
			margin: 0 auto;
			margin-bottom: 20px;
			display: flex;
			max-width: 301px;
			justify-content: center;
			width: 301px;
		}

		
		.content{
			position:relative;
		}
		.blur .mood-emoji-block,
		.blur .mood-percent__item{
			filter: blur(5px);
		}
		.blur:last-of-type::before{
			   
				
			    content: '';
				position: absolute;
				top: 50%;
				left: 50%;
				transform: translate(-50%, -50%);
				z-index: 10;
				display: inline-block;
				height: 97px;
				width: 76px;
				top: 50%;
				left: 50%;
				background-size: 100%;
			background-image:url('data:image/png;base64,');
		}
		.blur.showFirst .mood-emoji-block:first-of-type,
		.blur.showFirst .mood-percent__item:first-of-type{
			filter: blur(0px);
		}
		.mood-percent {
			display: inline-block;
			background-color: #fef8ca;
			margin-left: 30px;
			width: 70px;
			
			vertical-align: top;
			border-radius: 5px;
		}
		.mood-percent__item {
			font-family: Rubik, serif;
			font-size: 19px;
			line-height: 66px;
			font-weight: 300;
			width: 40px;
			text-align: center;
			font-family: Rubik, serif;
			padding: 18px 15px;
			border-bottom: 2px dashed #C2DFCB;

		}
		.mood-percent__item:first-of-type{
			line-height: 100px;
		}
		.mood-percent__item:last-of-type{
			border-bottom: none;
		}
		.mood-emoji {
			display: inline-block;
			width: 200px;
			padding-left: 20px;
			padding-top: 8px;
		}
		
		.mood-emoji-block__icon{
		
			font-weight: 700;
			font-size: 25px;
			line-height: 32.5px;
			padding-bottom: 5px;
			color: #155c82;
		}
		.mood-emoji-block__icon span{
			margin-left:5px;
		}
		.mood-emoji-block__times {
			font-weight: 400;
			font-size: 20px;
			line-height: 26px;
			padding-bottom: 30px;
			color: #5c4e8e;
		}
		footer {
			font-size: 22px;
			margin: auto auto 5px;
			color: #6FC7C3;
			bottom:0;
			position:absolute;
			width:100%;
			text-align:center;
		}

