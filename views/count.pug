- var header = "Ты отмечал настроение";
- var count = 7;

doctype 5
html(lang='ru')
	meta(charset="utf-8")
	title Some practice
	meta(name='viewport', content='width=device-width, initial-scale=1')
	link(href="https://fonts.googleapis.com/css2?family=Rubik:wght@300;400;500;700&display=swap" rel="stylesheet")
	style.
		html, body {
			height: 100%;
			width: 100%;
			margin: 0 !important;
			padding: 0 !important;
		}

		.container {
			display: flex;
			flex-direction: column;
			min-height: 100%;
			min-width: 100%;

			/** Here example of big image */
			background: url("data:image/png;base64, ") 100% 50% /cover no-repeat;
		}

		.header {
			font-family: Rubik, serif;
			font-style: normal;
			font-weight: bold;
			font-size: 47px;
			line-height: 130%;
			color: #FEF8CA;
			padding: 48px 25px 0;
			margin: 0 auto 50%;
			display: flex;
			max-width: 260px;
			justify-content: center;
		}

		.count-number {
			font-family: Rubik, serif;
			font-weight: 900;
			color: #F86968;
			display: flex;
			justify-content: center;
			font-size: 190px;
			line-height: 1;
		}

		.count span {
			font-size: 40px;
			transform: rotate(-26.65deg);
			display: block;
			font-family: Rubik, serif;
			font-weight: 500;
			color: #F86968;
			max-width: 80px;
			margin: 0 auto;
		}

		footer {
			font-size: 22px;
			font-family: Rubik, serif;
			margin: auto auto 5px;
			color: #FEF8CA;
		}

body
	div.container
		h1.header #{header}
		.count
			.count-number #{count}
			span раз
		footer Такое делает @pandajournal_bot

